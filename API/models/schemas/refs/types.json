{
    "$id": "refs/types.json",
    "definitions": {
        "color": {
            "pattern": "^[0-9a-fA-F]{6}$"
        },
        "metricImperialUnit": {
            "enum": ["mm", "cm", "m", "f"]
        },
        "vector2": {
            "pattern": "^\\s*{\\s*\"x\"\\s*:\\s*([-|+]?[0-9]*[.]?)?[0-9]+\\s*,\\s*\"y\"\\s*:\\s*([-|+]?[0-9]*[.]?)?[0-9]+\\s*}\\s*$"
        },
        "vector3": {
            "pattern": "^\\s*{\\s*\"x\"\\s*:\\s*([-|+]?[0-9]*[.]?)?[0-9]+\\s*,\\s*\"y\"\\s*:\\s*([-|+]?[0-9]*[.]?)?[0-9]+\\s*,\\s*\"z\"\\s*:\\s*([-|+]?[0-9]*[.]?)?[0-9]+\\s*}\\s*$"
        },
        "float": {
            "pattern": "^[-]?([0-9]*[.])?[0-9]+$"
        }
    }
}