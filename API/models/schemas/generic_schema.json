{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "OGrEE Generic object Schema",
  "$ref": "refs/base.json",
  "type": "object",
  "properties": {
    "attributes": {
      "type": "object",
      "properties": {
        "fbxModel": {
          "type": "string"
        },
        "height": {
          "$ref": "refs/types.json#/definitions/float"
        },
        "heightUnit": {
          "type": "string",
          "$ref": "refs/types.json#/definitions/metricImperialUnit"
        },
        "rotation": {
          "$ref": "refs/types.json#/definitions/vector3"
        },
        "posXYZ": {
          "$ref": "refs/types.json#/definitions/vector3"
        },
        "posXYUnit": {
          "type": "string",
          "enum": ["m", "t", "f"]
        },
        "posZUnit": {
          "type": "string",
          "$ref": "refs/types.json#/definitions/metricImperialUnit"
        },
        "size": {
          "format": "vector2"
        },
        "sizeUnit": {
          "type": "string",
          "$ref": "refs/types.json#/definitions/metricImperialUnit"
        },
        "type": {
          "type": "string"
        },
        "template": {
          "type": "string"
        },
        "shape": {
          "type": "string",
          "enum": [
            "cube",
            "sphere",
            "cylinder"
          ]
        },
        "color": {
          "type": "string",
          "$ref": "refs/types.json#/definitions/color"
        },
        "clearance": {
          "$ref": "refs/types.json#/definitions/clearanceVector"
        }
      },
      "required": [
        "height",
        "heightUnit",
        "rotation",
        "posXYZ",
        "posXYUnit",
        "size",
        "sizeUnit",
        "type",
        "shape"
      ]
    }
  },
  "examples": [
    {
      "attributes": {
        "height": "47",
        "heightUnit": "cm",
        "rotation": "[45, 45, 45]",
        "posXYZ": "[4.6666666666667, -2, 0]",
        "posXYUnit": "m",
        "size": "[80, 100.532442]",
        "sizeUnit": "cm",
        "template": "",
        "shape": "cube",
        "type": "box"
      },
      "category": "generic",
      "description": "A table",
      "domain": "domainTest",
      "name": "GeneticA",
      "parentId": "someRoomId"
    }
  ]
}